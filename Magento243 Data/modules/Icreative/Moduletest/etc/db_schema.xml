<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
   <table name="Icreative_faq" resource="default" engine="innodb" comment="ICreative FAQ">
      <column xsi:type="int" name="id" padding="7" unsigned="false" nullable="false" identity="true" comment="ID" />
      <!-- <constraint xsi:type="primary" referenceId="PRIMARY"/> -->
      <column xsi:type="varchar" name="question" nullable="false" length="255" comment="Question" />
      <column xsi:type="varchar" name="answer1" nullable="false" length="200" comment="Answer" disabled="true"/>
      <column xsi:type="text" name="answer" nullable="true" comment="Answer" />
      <column xsi:type="int" name="status" nullable="false" padding="7" comment="Status" />
      <column xsi:type="varchar" name="image" nullable="false" length="255" comment="Image" />
      <constraint xsi:type="primary" referenceId="PRIMARY">
         <column name="id" />
      </constraint>
   </table>
   <table name="observer_faq" resource="default" engine="innodb" comment="Observer Example FAQ">
      <column xsi:type="int" name="id" padding="7" unsigned="false" nullable="false" identity="true" comment="ID" />
      <column xsi:type="varchar" name="question" nullable="false" length="255" comment="Question" />
      <column xsi:type="text" name="answer" nullable="true" comment="Answer" />
      <column xsi:type="int" name="status" nullable="false" padding="7" comment="Status" />
      <constraint xsi:type="primary" referenceId="PRIMARY">
         <column name="id" />
      </constraint>
   </table>
</schema>


